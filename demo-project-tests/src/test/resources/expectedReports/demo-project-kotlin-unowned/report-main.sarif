{
    "$schema": "https://json.schemastore.org/sarif-2.1.0-rtm.4",
    "version": "x.y.z",
    "runs": [
        {
            "tool": {
                "driver": {
                    "name": "io.github.gmazzo.codeowners.CodeOwnersReportTask",
                    "informationUri": "https://github.com/gmazzo/gradle-codeowners-plugin",
                    "version": "x.y.z",
                    "rules": [
                        {
                            "id": "UnownedClassFile",
                            "shortDescription": {
                                "text": "Class file does not have a code owner"
                            }
                        }
                    ]
                }
            },
            "artifacts": [
                {
                    "first": "location",
                    "second": {
                        "uri": "file:<baseDir>/gradle-codeowners-plugin/demo-project-kotlin/unowned/"
                    }
                }
            ],
            "results": [
                {
                    "ruleId": "UnownedClassFile",
                    "level": "error",
                    "message": {
                        "text": "Class file does not have a code owner"
                    },
                    "locations": [
                        {
                            "physicalLocation": {
                                "artifactLocation": {
                                    "uri": "file:<baseDir>/gradle-codeowners-plugin/demo-project-kotlin/unowned/src/main/kotlin/org/test/kotlin/unowned/Unowned.kt"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    ]
}